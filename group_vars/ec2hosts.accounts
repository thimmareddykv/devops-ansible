[ssh_connection]
ssh_args=-o ControlPersist=15m -F ~/.ssh/config -q
scp_if_ssh=True
control_path = ~/.ssh/mux-%%r@%%h:%%p
ansible_ssh_user=ec2-user
ansible_ssh_private_key_file=~/.ssh/wfm-devops-root.pem 
ansible_distribution=CentOS

